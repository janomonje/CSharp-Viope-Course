/*
You are provided with a List of type string having the following names:

Torvalds, Musk, Gates, Buffet, Dorsey.

 

Modify the list so that the items are:

Jobs, Musk, McAfee, Buffet, Wozniak.

 

Hint: you can use any of the suitable List class methods to complete the task.


Example output:
Jobs
Musk
McAfee
Buffet
Wozniak
*/


using System;
using System.Collections.Genecric;

namespace Chapter_7
{
  Class Exercise_1
  {
    static void Main(string [] args)
    {
      List<string> names = new List<string> {"Torvalds", "Musk", "Gates", "Buffet", "Dorsey"};
      
      for(int i = 0; i < 5; i++)
      {
       if(names[i] == "Torvalds")
       {
         names[i] = "Jobs";
       }
       if(names[i] == "Gates")
       {
         names[i] = "McAfee";
       }
       if(names[i] == "Dorsey")
       {
         names[i] = "Wozniak";
       }
      }
      foreach (string name in names)
      {
       Console.WriteLine(name);
      }
    }
  }
}
